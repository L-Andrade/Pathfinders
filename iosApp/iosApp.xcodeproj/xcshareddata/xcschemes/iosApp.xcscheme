                  <BuildableReference
                     BuildableIdentifier = "primary"
                     BlueprintIdentifier = "B9DA97B02DC1472C00A4DA20"
                     BuildableName = "Pathfinders Debug.app"
                     BlueprintName = "iosApp"
                     ReferencedContainer = "container:iosApp.xcodeproj">
